<div class="col-12" data-fileResult-template style="display: none;">
    <div class="col-12" data-file>
        <div class="col-1" data-remove>
            <span class="fa fa-trash-alt" onclick="removeSelected(this)"></span>
        </div>
        <div class="col-2" data-img>
            <img src="" width="80" alt="sample" />
        </div>
        <div class="col-12 p2">
            <div class="col-lg-12 col-sm-12" data-file-info>
                <span data-file-name></span>
                <span data-file-size></span>
            </div><!-- File Info-->
            <div class="col-lg-12  col-sm-12 progress" data-progress style="display: none;">
                <div class="progress-bar progress-bar-danger"
                     role="progressbar"
                     data-progress-bar
                     aria-valuenow="0"
                     aria-valuemin="0"
                     aria-valuemax="100"
                     style="width: 0;">
                </div>
            </div><!-- Progress bar-->
            <div class="col-lg-12 col-sm-12" data-alert style="display: none;">
                <span class="label label-danger">size over 1.5MB</span>
            </div>
        </div>
    </div>
</div>